#include <stdio.h>

int main() {
    double A, B, C;
    double media;

    // Leitura das notas A, B e C
    scanf("%lf", &A);
    scanf("%lf", &B);
    scanf("%lf", &C);

    // Cálculo da média
    media = ((A * 2) + (B * 3) + (C * 5)) / 10;

    // Impressão da média
    printf("MEDIA = %.1lf\n", media);

    return 0;
}
